    "url": "https://github.com/horacehylee/gcalendar-cli.git"
  },
  "dependencies": {
    "axios": "0.21.2",
    "bluebird": "3.5.1",
    "chalk": "2.3.0",
    "class-transformer": "0.1.8",
    "cli-table3": "0.5.0",
    "date-fns": "1.29.0",
    "dotenv": "4.0.0",
    "googleapis": "^67.1.1",
    "inquirer": "^8.0.0",
    "lodash": "^4.17.10",
    "moment": "2.29.4",
    "node-emoji": "1.8.1",
    "opn": "5.1.0",
