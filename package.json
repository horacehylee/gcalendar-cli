    "@types/node-emoji": "1.8.0",
    "@types/opn": "^5.5.0",
    "@types/ora": "1.3.1",
    "@types/update-notifier": "1.0.2",
    "@types/yargs": "10.0.0",
    "backpack-core": "^0.8.4",
    "coveralls": "^3.0.0",
    "jest": "22",
    "lodash-ts-imports-loader": "1.1.2",
